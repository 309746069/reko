{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceRoot}/generated/main.js",
			"stopOnEntry": false,
			"args": [],
			"env": {},
			"sourceMaps": true,
			"outFiles": [
				"${workspaceRoot}/generated/**/*.js"
			],
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
			"runtimeArgs": [
				"--remote-debugging-port=9222"
			],
			"console": "integratedTerminal",
			"preLaunchTask": "build"
		}
	]
}